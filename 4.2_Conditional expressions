void main(){
 
  // Conditional expressions
  int a = 2;
  int b = 3;
  if(a<b){
    print("$a is smaller");
  } else {
    print("$b is smaller");
  }
          //OR
  
  a<b? print("$a is smaller"):print("$b is smaller");

//TERNAL OPERATOR  
String name = "tom";
  String nameToprint = name?? "Guest User"; //Guest user is for null value
  print(nameToprint);
}
