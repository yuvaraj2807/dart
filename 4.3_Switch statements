// String and int only used in switch statements

void main(){
  String grade = 'A';
  switch (grade){
    case 'A':
      print("Excellant");
      break;
      
    case 'B':
      print("very good");
      break;
      
    case'C':
      print("good");
      break;
      
    default:
      print("FAIL");
  }
}
