void main(){
  
  //IF and ELSE statement
  
  var salary = 15000;
  if(salary>20000){
  print("You got promotion.Congradulations");
  } else { 
  print("You should work hard");
}

//IF ELSE IF Ladder statements

  var marks = 70;
  
  if (marks>=90 && marks<100){
    print("DISTINCTION");
  } else if(marks>=45 && marks<90){
    print("PASS");
  } else {
    print("FAIL");
  }
}
